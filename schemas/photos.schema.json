{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "array",
    "additionalItems": false,
    "items": {
        "$id": "#/items",
        "anyOf": [
            {
                "$id": "#/items/anyOf/0",
                "type": "object",
                "required": [
                    "albumId",
                    "id",
                    "title",
                    "url",
                    "thumbnailUrl"
                ],
                "properties": {
                    "albumId": {
                        "$id": "#/items/anyOf/0/properties/albumId",
                        "type": "integer"
                    },
                    "id": {
                        "$id": "#/items/anyOf/0/properties/id",
                        "type": "integer"
                    },
                    "title": {
                        "$id": "#/items/anyOf/0/properties/title",
                        "type": "string"
                    },
                    "url": {
                        "$id": "#/items/anyOf/0/properties/url",
                        "type": "string"
                    },
                    "thumbnailUrl": {
                        "$id": "#/items/anyOf/0/properties/thumbnailUrl",
                        "type": "string"
                    }
                },
                "additionalProperties": false
            }
        ]
    }
}
